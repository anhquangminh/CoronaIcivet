
@{
    ViewBag.Title = "InsertExcel";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br />
<div class="card">
    <div class="card-header">
        <h4>Tải lên thư mục excel</h4>
    </div>
    <div class="card-body">
        <div class="row">
            <form class="col-12">
                <div class="form-group">
                    <label class="control-label col-md-12">Nhiệt độ hiện tại :</label>
                    <div class="col-md-12">
                        <a href="#">Tải xuống file mẫu </a>
                    </div>
                </div>
                <hr />
                <div class="form-group">
                    <label class="control-label col-md-12">Tải lên file excel :</label>
                    <div class="col-md-12">
                        <input name="file" class="form-control" type="file" placeholder="" style="padding:5px;" />
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-md-3">
                        <input type="submit" class="btn-default " value="Tải lên" />
                    </div>
                    
                </div>
            </form>
        </div>
    </div>
</div>

